{
  "name": "order-service",
  "version": "0.0.1",
  "description": "Manages highly concurrent order propagation thru the fulfillment cycle",
  "author": "P&F",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "console": "node --inspect ./node_modules/.bin/ts-node -r tsconfig-paths/register -r dotenv/config bin/console",
    "prebuild": "rimraf dist",
    "build": "yarn generate && node ./node_modules/.bin/nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "setup": "yarn db:migrate",
    "start": "node -r dotenv/config ./node_modules/.bin/nest start",
    "prestart:dev": "rimraf dist",
    "start:dev": "node -r dotenv/config ./node_modules/.bin/nest start --watch",
    "start:debug": "node -r dotenv/config ./node_modules/.bin/nest start --debug --watch",
    "start:prod": "node -r dotenv/config ./dist/src/main.js",
    "lint": "eslint \"{bin,src,test}/**/*.ts\"",
    "test": "yarn jest",
    "test:coverage": "yarn jest --coverage",
    "test:coverage:report": "yarn jest --maxWorkers=2 --coverage --coverageReporters=text-lcov | coveralls",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e:circle": "dotenv yarn db:reset:force && dotenv jest",
    "test:e2e": "yarn generate && jest --coverage --runInBand && yarn db:reset:force",
    "test:watch": "yarn jest --watch",
    "generate": "prisma generate",
    "db:newmigration": "prisma migrate dev --create-only",
    "db:migrate": "prisma migrate deploy",
    "db:reseed": "prisma db seed",
    "db:reset": "prisma migrate reset",
    "db:reset:force": "prisma migrate reset --force --skip-seed",
    "db:reset:force:reseed": "prisma migrate reset --force",
    "db:rollback": "prisma migrate resolve --rolled-back",
    "safelymigrate": "npx @thirtymadison/safely-migrate",
    "prepare": "husky install"
  },
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  },
  "engines": {
    "node": "^14 || ^16"
  },
  "dependencies": {
    "@apollo/federation": "^0.29.0",
    "@faker-js/faker": "^7.6.0",
    "@medplum/core": "^2.0.2",
    "@medplum/fhirtypes": "^2.0.3",
    "@nestjs/cli": "^9.1.4",
    "@nestjs/common": "^8.0.6",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.0.6",
    "@nestjs/graphql": "^8.0.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/microservices": "^9.0.2",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.2",
    "@nestjs/terminus": "^9.0.0",
    "@prisma/client": "^4.11.0",
    "@sentry/node": "^7.15.0",
    "@thirtymadison/apollo-client": "^2.0.0",
    "@thirtymadison/data-events-lib": "^0.0.32",
    "@thirtymadison/drug-database-sdk": "1.0.11",
    "@thirtymadison/environment-utils": "^1.0.0",
    "@thirtymadison/flags": "^1.0.0",
    "@thirtymadison/graphql-types": "^0.669.0",
    "@thirtymadison/nestjs-auth": "^0.1.1",
    "@thirtymadison/nestjs-flags": "^0.0.5",
    "@thirtymadison/nestjs-prisma": "^0.0.4",
    "@thirtymadison/nestjs-vault": "^0.0.2",
    "@thirtymadison/safely-migrate": "^1.1.2",
    "apollo-server": "^3.10.3",
    "apollo-server-errors": "^3.3.1",
    "apollo-server-express": "^2.4.2",
    "cache-manager": "^5.1.1",
    "chalk": "^4.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.0",
    "connect-timeout": "^1.9.0",
    "dd-trace": "^2.18.0",
    "dotenv": "^16.0.3",
    "fast-json-stringify": "^5.4.0",
    "graphql": "^15.7.2",
    "graphql-tools": "^8.3.0",
    "helmet": "^6.0.0",
    "hot-shots": "^9.1.0",
    "kafkajs": "^2.2.3",
    "lodash.get": "^4.4.2",
    "nestjs-pino": "^1.4.0",
    "node-fetch": "2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^8.1.1",
    "@types/cache-manager": "^4.0.2",
    "@types/express": "^4.17.8",
    "@types/faker": "^6.6.9",
    "@types/jest": "^26.0.15",
    "@types/node": "^16",
    "@types/passport-jwt": "^3.0.7",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.10",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "coveralls": "^3.1.1",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^8.0.3",
    "jest": "^26.6.3",
    "jest-mock-extended": "^3.0.1",
    "lint-staged": "^10.5.4",
    "pino-pretty": "^9.1.1",
    "prettier": "^2.5.1",
    "prisma": "^4.11.0",
    "purdy": "^3.5.1",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint"
  },
  "@thirtymadison:gatewayGraphQLQueryPaths": [],
  "@thirtymadison:hasuraGraphQLQueryPaths": []
}
